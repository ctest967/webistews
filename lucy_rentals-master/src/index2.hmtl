}

body {
	padding 0;
	margin: 0;
}

h1, h2, h3, h4, h5, h6 {
	font-family: 'Audiowide', cursive;
	letter-spacing: 4px;
}

p {
	line-height: 1.5em;
	letter-spacing: 1px;
	font-family: 'Exo 2', sans-serif;
}

*:focus {
	outline: none;
}

.wrapper {
	padding: 0;
	margin:0;
}

img {
	width: 100%;
	height: auto;
	box-shadow: 0 8px 30px 2px rgba(0, 0, 0, .5);
}

.img-container {
	max-width: 800px;
	height: auto;
}

.about, .s1, .s2, .s3, .s4, .s5, .s6 {
	width: 100%;
	height: auto;
	padding: 100px 50px;
	color: #303030;
}

.btn {
	display: inline-block;
	padding: 15px 18px;
	margin: 25px 0;
	border: 1px solid #00CCA0;
	background: transparent;
	color: #00CCA0;
	border-radius: 50px;
	text-transform: uppercase;
	transition: all .35s;
}

.btn:hover {
	background: #00CCA0;
	color: #4CFFD9;
}

/* 
==================================================
Grid Layout
================================================== 
*/

.row {
	display: -webkit-box;
	display: -ms-flexbox;
	display: flex;
	margin: 0 0 20px 0;
}

.col {
	padding: 20px;
}

.row .col {
	-webkit-box-flex: 1;
	-ms-flex: 1;
	flex: 1;
}

.row-halves, .row-thirds, .row-fourths {
	display: -webkit-box;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-ms-flex-pack: justify;
	justify-content: space-between;
}

.row-halves .col {
	width: 49%;
}

.row-thirds .col {
	width: 32%;
}

.row-fourths .col {
	width: 24%;
}

/* 
==================================================
Top Bar
================================================== 
*/

.top-bar {
	position: fixed;
	top: 0;
	z-index: 1; 
	text-align: right;
	padding: 8px 50px;
	width: 100%;
	background: rgba(0, 0, 0, 0.2);
	font-size: .8em;
	border-bottom: 1px solid rgba(0, 0, 0, 0.2);
}

.top-bar ul {
	padding: 0;
	margin: 0;
	list-style: none;
}

.top-bar ul li {
	display: inline;
	padding: 0 10px;
	margin: 0;
	color: red;
}

/* 
==================================================
Standard Nav
================================================== 
*/

.standard-nav {
	position: fixed;
	z-index: 1;
	top: 0px;
	width: 100%;
	background: #168BC3;
	padding: 20px 10px;
	text-align: center;
}

.standard-nav a {
	text-decoration: none;
	color: #eee;
	padding: 0 30px;
	text-transform: uppercase;
	letter-spacing: 0px;
	border-left: 1px solid #424242;
	transition: all .35s;
}

.standard-nav a:last-child {
	border-right: 1px solid #424242;
}

.standard-nav a:hover {
	color: #00CCA0;
}


/*
==================================================
Mobile Nav
================================================== 
*/

button {
	background: none;
	border: none;
	margin: 20px;
}

button:focus {
	outline: none;
}

.contain {
	position:relative;
	display: inline-block;
	cursor: pointer;
	z-index: 9999;
}

.change .container {
	transform: rotate(90deg);
}

.brand {
	color: #fff;
}

.bar1,
.bar2,
.bar3 {
	width: 25px;
	height: 2px;
	background-color: #fff;
	border-radius: 2px;
	margin: 4px 0;
	transition: 0.4s;
}

.change .bar1 {
	-webkit-transform: rotate(-45deg) translate(-6px, 6.5px);
	transform: rotate(-45deg) translate(-5.5px, 4.5px);
	top: 50px;
	
	background: #E00049;
}

.change .bar2 {
	position: relative;
	-webkit-transform: translate(30px, 0px); 
	transform: translate(30px, 0px); 
	background: #E00049;
	opacity: 0;
}

.change .bar3 {
	-webkit-transform: rotate(45deg) translate(-7px, -8px);
	transform: rotate(45deg) translate(-4px, -3px);
	background: #E00049;
}


.mobile-navbar {
	position: fixed;
	z-index:3;
	width: 100%;
	height: 58px;
	display: none;
	justify-content: space-between;
	align-content: center;
	align-items: center; 
	padding: 0 40px;
	background-color: #222;	
}

button:focus {
	outline: 0;
}

.open-menu {
	cursor: pointer;
	transition: transform 0.3s;
	color: #fff;
}

.button:hover,
.open-menu:hover {
	transform: scale(1.1);
} 
/* Overlay */

.overlay {
	position: fixed;
	z-index: 2;
	background: rgba(0, 0, 0, 0.8);
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	opacity: 0;
	visibility: hidden;
	transition: opacity 0.35s, visibility 0.35s, height 0.35s;
	overflow: auto;
}

.overlay.open {
	opacity: 1;
	visibility: visible;
	height: 100%;
}

.overlay nav {
	position: relative;
	height: 300px;
	top: 300px;
	transform: translateY(-50%);
	text-align: center;
}

.overlay ul {
	list-style: none;
	padding: 0;
	margin: 0 auto;
	display: inline-block;
	position: relative;
	left: 1%;
	height: 100%;
}

.overlay ul li {
	vertical-align: middle;
	display: block;
	position: relative;
	opacity: 0;
	margin-bottom: 35px;
	font-family: "Exo 2", sans-serif;
	font-size: 1.5em;
	font-weight: 400;
	text-transform: uppercase;
	letter-spacing: 4px;
}

.overlay ul li a {
	display: block;
	position: relative;
	color: #fff;
	text-decoration: none;
	overflow: hidden;
	transition: all 0.4s;
}

.overlay ul li a:after {
	content: "";
	position: absolute;
	top: 50%;
	left: 43.5%;
	width: 0%;
	transform: translateX(-50%);
	height: 2px;
	background: #fff;
	transition: 0.35s;
}

.overlay ul li a:hover,
.overlay ul li a:focus,
.overlay ul li a:active {
	color: #fff;
}

.overlay ul li a:hover:after,
.overlay ul li a:focus:after,
.overlay ul li a:active:after {
	width: 100%;
	background: #00CCA0;
}

.overlay.open li {
	animation: fadeInRight 0.5s ease forwards;
	animation-delay: 0.2s;
}

.overlay.open li:nth-of-type(2) {
	animation-delay: 0.25s;
}

.overlay.open li:nth-of-type(3) {
	animation-delay: 0.3s;
}

.overlay.open li:nth-of-type(4) {
	animation-delay: 0.45s;
}

.overlay.open li:nth-of-type(5) {
	animation-delay: 0.5s;
}

/* Animation for menu fade in */

@keyframes fadeInRight {
	0% {
		opacity: 0;
		left: 20%;
	}
	100% {
		opacity: 1;
		left: 0;
	}
}


